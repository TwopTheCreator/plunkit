# top-most EditorConfig file
root = true

# Default settings for all files
[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true


# Override for JSON files
[*.json]
indent_size = 2

# Override for Python files
[*.py]
max_line_length = 88
